{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["@nuxt/types", "@nuxtjs/strapi"],
    "rootDir": "."
  },
  "include": ["./**/*.ts", "./**/*.vue"],
  "exclude": ["node_modules", ".nuxt", "dist"]
}